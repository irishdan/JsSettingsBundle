{
    "name": "irishdan/js-settings-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for injecting data as javascript into a page",
    "keywords": [
        "php",
        "javascript",
        "symfony",
        "twig"
    ],
    "homepage": "https://github.com/irishdan/JsSettingsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Byrne",
            "email": "danielbyrne@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/property-access": "~3.4|~4.0"
    },
    "autoload": {
        "psr-4": {
            "IrishDan\\JsSettingsBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    }
}
